



Select c.supplier_cbn, c.type, h.*,  PL.PL_CODE, PL.PL_DESCRIPTION from wizadmin.company_profile c, wizadmin.wty_header h   


, wizadmin.product_info p1,  wizadmin.product_line pl 


where c.cust_id=h.cust_id 


and P1.PRODUCT_NUMBER=H.PRODUCT_NUMBER 


and P1.PRODUCT_LINE_ID=PL.PRODUCT_LINE_ID 


/*  Date needs to be Updated  */

and H.declared_DATE between  '01-juL-2019' and '31-juL-2019' 


and c.type in ('COMP_HP_SP','COMP_ASP') 


and h.wty_type ='WTYHTYPE_LABORCLAIM' 
